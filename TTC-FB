# HÃ m delay chung vá»›i tham sá»‘ cho hÃ nh Ä‘á»™ng vÃ  thá»i gian
def delay_action(second, action_text, is_error=False):
    for i in range(int(second * 10), 0, -1):
        icon = frames[i % len(frames)]  # Chá»n icon theo bÆ°á»›c
        color = RED if is_error else CYAN if i % 2 == 0 else BLUE
        bracket_color = YELLOW if i % 2 == 0 else MAGENTA
        print(f"{color}{icon} {action_text} {bracket_color}[{i//10}.{i%10}s] {RESET}", end="\r")
        time.sleep(0.1)
    print(" " * 60, end="\r")   
def delay(second):
    delay_action(second, "Äang cháº¡y job")
def delay_laplai(second):
    delay_action(second, "Äang láº¥y job")
def delay_die(second):
    delay_action(second, "Job die => Äang bá» qua", is_error=True)
def delay_anti(second):
    delay_action(second, "Äang cháº¡y antiband")
def delay_xoa(second):
    delay_action(second, "Äang Chuáº©n Bá»‹ TÃ i NguyÃªn VÃ o Tool") 
def delay_kt(second):
    delay_action(second, "Tá»± kiá»ƒm tra vÃ  Ä‘á»•i acc cho Ä‘Ãºng admin xin cáº£m Æ¡n....") 
# Táº¡o profile
def login_ttc(driver, username, password):
    driver.get("https://tuongtaccheo.com/home.php")
    driver.find_element(By.NAME, "username").send_keys(username)
    driver.find_element(By.NAME, "password").send_keys(password)
    driver.find_element(By.NAME, "submit").click()
    time.sleep(3)

def tao_profile_moi():
    index = 1
    while True:
        new_profile_path = os.path.join(base_path, f"chrome_profile_{index}")
        if not os.path.exists(new_profile_path):
            break
        index += 1

    print(f"{CYAN}â¡ï¸ Äang táº¡o profile chrome_profile_{index}, vui lÃ²ng Ä‘Äƒng nháº­p GoLike{RESET}")
    driver = kt_driver(new_profile_path)
    driver.set_window_size(500, 700)
    delay_kt(2)
    driver.get("https://tuongtaccheo.com/home.php")
    user_name = input("ğŸ‘‰ Nháº­p User: ")
    pass_name = input("ğŸ‘‰ Nháº­p Pass: ")
    user_element = driver.find_element(By.NAME, 'username')
    user_element.send_keys(user_name)
    user = user_element.get_attribute("value") # Láº¥y dá»¯ liá»‡u user
    delay_kt(0.5)
    pass_element = driver.find_element(By.NAME, 'password')
    pass_element.send_keys(pass_name)
    passs = pass_element.get_attribute("value") # Láº¥y dá»¯ liá»‡u pass
    print(f"user {user} pass {passs}")
    delay_kt(0.5)
    driver.find_element(By.NAME, 'submit').click()
    print("Äang lÆ°u tk mk vÃ o file account.txt", end= "\r")
    save_path = "account_info.txt" # 
    if not os.path.exists(save_path): # Táº¡o file
        print(f"Táº¡o má»›i file: {save_path}")
    with open(save_path, 'a') as f: # LÆ°u file 
        f.write(f"Username: {user}| Password: {passs}\n")
    print(""*20, end="\r")
    delay_kt(2)
    driver.execute_script("window.open('https://m.facebook.com/login');")
    user_fb = input("ğŸ‘‰ Nháº­p User FB: ")
    pass_fb = input("ğŸ‘‰ Nháº­p Pass FB: ")
    all_tab = driver.window_handles
    driver.switch_to.window(all_tab[1])
    delay_kt(2)
    driver.find_element(By.NAME, 'email').send_keys(user_fb)
    delay_kt(2)
    driver.find_element(By.NAME, 'pass').send_keys(pass_fb)
    delay_kt(2)
    driver.find_element(By.NAME, 'login').click()
    while True:
        driver.get("https://www.facebook.com/friends/")
        delay_kt(2)
        if driver.current_url.startswith("https://www.facebook.com/friends/"):
            print(f"{Fore.GREEN}[âœ…] ÄÃ£ Ä‘Äƒng nháº­p Facebook{Style.RESET_ALL}")
            break
        else:
            input("Vui lÃ²ng Ä‘Äƒng nháº­p Facebook tay, sau Ä‘Ã³ báº¥m Enter...")
            continue
    driver.quit()
    print(f"{GREEN}âœ… ÄÃ£ táº¡o vÃ  lÆ°u chrome_profile_{index}{RESET}")
    return new_profile_path 
base_path = os.path.dirname(os.path.abspath(__file__))
profiles = []        
def load_profiles_from_file():
    profiles = []
    if os.path.exists('profiles.txt'):
        with open('profiles.txt', 'r') as file:     
            profiles = [line.strip().split("\\")[-1] for line in file.readlines()]
    return profiles
profiles = load_profiles_from_file()
def save_profiles_to_file(profiles):
    with open('profiles.txt', 'w') as file:
        for profile in profiles:
            file.write(f"{profile}\n")

def kiem_tra_profile(profiles):
    os.system("cls")
    print("")
    console.print("[bold magenta]                      Welcome to[/bold magenta][bold yellow] the ğ“‘ğ“ğ“¢ğ“¢ [/bold yellow]")
    console.print("[bold magenta]                 â•šâ•â•¦â•â•â•â•â•â•â•â•â•â•â•â•[/bold magenta][bold yellow]â•â•â•â•â•â•â•â•â•â•â•¦â•â•")
    console.print("[bold magenta]â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•[ Golike Fa[/bold magenta][bold yellow]cebook ]â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•[/bold yellow]")
    print("")# 18
    console.print("[bold magenta]                  â•”â•â•â•â•â•â•â•â•â•â•â•â•â•[/bold magenta][bold yellow]â•â•â•â•â•â•â•â•â•â•â•â•â•—       ")
    console.print("[bold magenta]              â•”â•â•â•â• Danh sÃ¡ch cÃ¡[/bold magenta][bold yellow]c tÃ i khoáº£n â•šâ•â•â•â•—")
    console.print("[bold magenta]             â•™â•‘                               [/bold magenta][bold yellow]  â•‘â•œ ")
    for idx, profile in enumerate(profiles, start=1):
        console.print(f"            [bold magenta] â•™â•‘  [/bold magenta]    [bold yellow][{idx}][/bold yellow] [bold magenta]{profile}[/bold magenta]       [bold yellow]â•‘â•œ")
    console.print("[bold magenta]             â•™â•‘                               [/bold magenta][bold yellow]  â•‘â•œ ")
    console.print("[bold magenta]              â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•[/bold magenta][bold yellow]â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•       ")
    print("")
    lua_chon = console.input("  [[bold yellow]ğ“‘ğ“ğ“¢ğ“¢[/bold yellow]|[bold magenta]Nháº­p sÃ´[/bold magenta]][bold green]#   ").strip()
    if lua_chon.lower() == 'x':
        return
    
    try:
        lua_chon = int(lua_chon)
        if 1 <= lua_chon <= len(profiles):
            profile_path = profiles[lua_chon - 1]
            print(f"{CYAN}â¡ï¸ Äang kiá»ƒm tra tÃ i khoáº£n: {profile_path}{RESET}", end="\r")
            
            driver = kt_driver(profile_path)
            driver.set_window_size(500,700)
            # Kiá»ƒm tra GoLike
            driver.get("https://tuongtaccheo.com/home.php")
            delay_kt(1)
            driver.execute_script("window.open('https://m.facebook.com/login');")
            # Kiá»ƒm tra Facebook
            all_windows = driver.window_handles
            driver.switch_to.window(all_windows[1])
            while True:
                driver.get("https://www.facebook.com/friends/")
                delay_kt(2)
                if driver.current_url.startswith("https://www.facebook.com/friends/"):
                    print(f"{Fore.GREEN}[âœ…] ÄÃ£ Ä‘Äƒng nháº­p Facebook: {profile_path}{Style.RESET_ALL}")
                    break
                else:
                    input("Vui lÃ²ng Ä‘Äƒng nháº­p Facebook, sau Ä‘Ã³ báº¥m Enter...")
                    continue
            input("KhÃ´ng thay Ä‘á»•i gÃ¬ ná»¯a thÃ¬ báº¥m enter rá»“i báº¥m 3 !")
            driver.quit()
        else:
            print(f"{RED}âš ï¸ Sá»‘ tÃ i khoáº£n khÃ´ng há»£p lá»‡!{RESET}")
    except ValueError:
        print(f"{RED}âš ï¸ Vui lÃ²ng nháº­p sá»‘ há»£p lá»‡!{RESET}")
# Láº¥y dá»¯ liá»‡u
def lay_du_lieu_ttc(file_path="account_info.txt"):
    accounts = []

    try:
        # Má»Ÿ file vÃ  Ä‘á»c cÃ¡c dÃ²ng
        with open(file_path, 'r') as f:
            lines = f.readlines()

        for line in lines:
            line = line.strip()  # Loáº¡i bá» khoáº£ng tráº¯ng Ä‘áº§u vÃ  cuá»‘i dÃ²ng
            if line:  # Kiá»ƒm tra dÃ²ng khÃ´ng trá»‘ng
                try:
                    # TÃ¡ch theo dáº¥u "|"
                    parts = line.split("|")

                    # Kiá»ƒm tra náº¿u dÃ²ng cÃ³ Ä‘á»§ 2 pháº§n: Username vÃ  Password
                    if len(parts) == 2:
                        username_part = parts[0].split(":")  # Pháº§n chá»©a Username
                        password_part = parts[1].split(":")  # Pháº§n chá»©a Password

                        # Kiá»ƒm tra vÃ  láº¥y username vÃ  password
                        if len(username_part) > 1 and len(password_part) > 1:
                            username = username_part[1].strip()  # Láº¥y pháº§n sau dáº¥u ":"
                            password = password_part[1].strip()  # Láº¥y pháº§n sau dáº¥u ":"

                            # ThÃªm vÃ o danh sÃ¡ch náº¿u username vÃ  password há»£p lá»‡
                            accounts.append((username, password))

                except Exception as e:
                    print(f"[âŒ] Lá»—i khi xá»­ lÃ½ dÃ²ng: '{line}': {e}")
                    continue  # Bá» qua dÃ²ng lá»—i vÃ  tiáº¿p tá»¥c vá»›i dÃ²ng tiáº¿p theo
    except Exception as e:
        print(f"[âŒ] Lá»—i khi Ä‘á»c file: {e}")

    return accounts


# LÃ m job like vip
tongxu = 0
biendem = 0
def lam_job_like_vip(driver,index):
    global tongxu, biendem
    try:
        driver.get("https://tuongtaccheo.com/kiemtien/likepostvipcheo/")
        driver.execute_script("document.body.style.zoom = '0.25';")
        driver.execute_script("document.body.style.zoom = '0.25';")
        delay_laplai(5)
        try:
            #tai = driver.find_element(By.ID, 'tailai')
            driver.find_element(By.XPATH ,'/html/body/div/div/div[2]/div/div[1]/div/div[1]/div/div').click()
        except:
            try:
                driver.find_element(By.XPATH, '/html/body/div/div/div[2]/div/div[1]/div/div[1]/div/div/button').click()
            except:
                try:
                    driver.find_element(By.XPATH, '/html/body/div/div/div[2]/div/div[1]/div/div[2]/div/div/button').click()
                except:
                    return 

        delay(15)
        all_tab = driver.window_handles
        driver.switch_to.window(all_tab[1])
        driver.execute_script("window.scrollBy(0, 500);")
        driver.execute_script("window.scrollBy(0, -500);")
        driver.execute_script("window.scrollBy(0, 500);")
        driver.execute_script("window.scrollBy(0, -500);")
        driver.execute_script("window.scrollBy(0, 500);")  
        driver.execute_script("window.scrollBy(0, -500);")
        found = False 
        try:
            try:
                # like
                modal = driver.find_element(By.XPATH, '//div[@role="dialog"]//div[@aria-label="ÄÃ³ng"]').find_element(By.XPATH, "./ancestor::div[@role='dialog']")
                like = WebDriverWait(modal, 5).until(EC.presence_of_element_located((By.XPATH, './/div[@aria-label="ThÃ­ch"]')))
            except:
                like = driver.find_element(By.XPATH, '//div[@aria-label="ThÃ­ch"]')
            try:
                found = True
                ActionChains(driver).move_to_element(like).perform()
                delay(2)
                ActionChains(driver).move_to_element(like).click().perform()
                delay(1)
            except:
                time.sleep(0.1)
            if found:
                handles = driver.window_handles
                driver.switch_to.window(handles[0])
                sodu_xpath = "/html/body/div[1]/div/nav/div/div[2]/ul[2]/li[2]/a/strong"
                # Kiá»ƒm tra sá»‘ xu
                oldsodu = WebDriverWait(driver, 10).until(EC.presence_of_element_located((By.XPATH, sodu_xpath)))
                soducu = int(oldsodu.text.replace(",", ""))
                driver.find_element(By.XPATH, '/html/body/div/div/div[2]/div/div[1]/div/div[1]/div/div/button').click()
                delay(10)
                sodumoi = int(oldsodu.text.replace(",", ""))
                handles = driver.window_handles
                driver.switch_to.window(handles[1])
                text_to_find = 'Giá» báº¡n chÆ°a dÃ¹ng Ä‘Æ°á»£c tÃ­nh nÄƒng nÃ y'
                try:
                    WebDriverWait(driver, 1).until(EC.presence_of_element_located((By.XPATH, f"//*[contains(text(), '{text_to_find}')]")))
                    return False
                except Exception as e:
                    time.sleep(0.5)
                try:
                    ActionChains(driver).move_to_element(like).click().perform()
                except:
                    
                    try:
                        modal = driver.find_element(By.XPATH, '//div[@role="dialog"]//div[@aria-label="ÄÃ³ng"]').find_element(By.XPATH, "./ancestor::div[@role='dialog']")
                        like = WebDriverWait(modal, 5).until(EC.presence_of_element_located((By.XPATH, './/div[@aria-label="ThÃ­ch"]')))
                    except:
                        like = driver.find_element(By.XPATH, '//div[@aria-label="ThÃ­ch"]')
                    ActionChains(driver).move_to_element(like).click().perform()
                handles = driver.window_handles
                driver.switch_to.window(handles[1])
                driver.close()
                driver.switch_to.window(handles[0])
                if sodumoi > soducu :
                    biendem +=1
                    tongxu +=1100
                    
                    print(f" [Luá»“ng {index}] {CYAN}| {biendem} |  FACEBOOK  | {RED} Live Vip   {RESET} |{GREEN} +1100 xu{RESET} | {YELLOW}Tá»•ng: {tongxu}{RESET} | {MAGENTA}Xu: {sodumoi}{RESET} | {BLUE}Time: {datetime.now().strftime('%H:%M:%S')}{RESET} | ")
                else:
                    print("Job lá»—i", end ="\r")
            else:
                handles = driver.window_handles
                driver.switch_to.window(handles[1])
                driver.close()
                driver.switch_to.window(handles[0])
                delay(1)
                sodu_xpath = "/html/body/div[1]/div/nav/div/div[2]/ul[2]/li[2]/a/strong"
                # Kiá»ƒm tra sá»‘ xu
                oldsodu = WebDriverWait(driver, 10).until(EC.presence_of_element_located((By.XPATH, sodu_xpath)))
                soducu = int(oldsodu.text.replace(",", ""))
                driver.find_element(By.XPATH, '/html/body/div/div/div[2]/div/div[1]/div/div[1]/div/div/button').click()
                delay(10)
                sodumoi = int(oldsodu.text.replace(",", ""))
                if sodumoi > soducu :
                    biendem +=1
                    tongxu += 1100
                    print(f" [Luá»“ng {index}] {CYAN}| {biendem} |  FACEBOOK  | {RED} Live Vip   {RESET} |{GREEN} +1100 xu{RESET} | {YELLOW}Tá»•ng: {tongxu}{RESET} | {MAGENTA}Xu: {sodumoi}{RESET} | {BLUE}Time: {datetime.now().strftime('%H:%M:%S')}{RESET} | ")

                else:
                    print("Job lá»—i", end ="\r")
        except:
            handles = driver.window_handles
            try:
                driver.switch_to.window(handles[1])
                driver.close()
            except:
                time.sleep(0.2)
            driver.switch_to.window(handles[0])
            time.sleep(0.2)
    except Exception as e:
        handles = driver.window_handles
        try:
            driver.switch_to.window(handles[1])
            driver.close()
        except:
            time.sleep(0.2)
        driver.switch_to.window(handles[0])
def lam_job_like_thuong(driver,index):
    global tongxu, biendem
    try:
        driver.get("https://tuongtaccheo.com/kiemtien/likepostvipre/")
        driver.execute_script("document.body.style.zoom = '0.25';")
        driver.execute_script("document.body.style.zoom = '0.25';")
        delay_laplai(5)
        try:
            #tai = driver.find_element(By.ID, 'tailai')
            driver.find_element(By.XPATH ,'/html/body/div/div/div[2]/div/div[1]/div/div[1]/div/div').click()
